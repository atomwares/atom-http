{
  "name": "atomwares/atom-http",
  "type": "library",
  "description": "PSR-7, PSR-15 (draft) and PSR-17 (draft) implementation by Atom Framework",
  "keywords": [
    "atom",
    "atomframework",
    "php",
    "http",
    "http-message",
    "http-middleware",
    "http-factory",
    "psr-7",
    "psr-15",
    "psr-17"
  ],
  "license": "MIT",
  "authors": [{
    "name": "Safarov Alisher",
    "email": "alisher.safarov@outlook.com",
    "homepage": "http://atomframework.net"
  }],
  "require": {
    "php": ">=7.0.0",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "http-interop/http-factory": "^0.3.0",
    "fig/http-message-util": "^1.1"
  },
  "provide": {
    "psr/http-message-implementation": "1.0"
  },
  "autoload": {
    "psr-4": {
      "Atom\\Http\\": "src/"
    }
  }
}
